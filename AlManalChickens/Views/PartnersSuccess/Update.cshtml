@model AlManalChickens.Domain.ViewModel.PartnersSuccess.GetPartnersSuccessById
<div class="card-box">

    <h2 class="header-title m-t-0 m-b-30">تعديل شريك</h2>

    <form asp-action="Update" method="post" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label asp-for="Name" class="control-label">الاسم </label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
            </div>

            <div class="col-md-3">
                <div class="form-group">
                    <label asp-for="NewImage" class="control-label">الصوره</label>
                    <input asp-for="NewImage" class="form-control" />
                    <span asp-validation-for="NewImage" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Image" class="control-label">الصورة الحاليه</label>
                    <div class="col-md-10">
                        <img src="@Model.Image" style="width:100px; height:100px; " />
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <input type="submit" value="تعديل" class="btn-new-style btn-rounded" />
                </div>
            </div>
            <div class="col-md-12">
                <div>
                    <a asp-action="Index" class="btn-black">العودة للرئيسية</a>
                </div>
            </div>
        </div>

    </form>


</div>